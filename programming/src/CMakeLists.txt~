project(raider)

cmake_minimum_required(VERSION 2.6)

set(TRUNK_DIR "${CMAKE_CURRENT_SOURCE_DIR}")
set(RAIDER_BIN_DIR "${SRC_DIR}/bin")
set(RAIDER_TESTS_DIR "${SRC_DIR}/tests")

add_subdirectory(libraries)
add_subdirectory(test)


